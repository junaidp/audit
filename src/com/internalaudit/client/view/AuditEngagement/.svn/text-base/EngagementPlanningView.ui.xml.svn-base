<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:HTMLPanel height="600px">
			
	<g:VerticalPanel ui:field="assignObjUI"> 
		<g:HTMLPanel>
		<div>
			<g:Label text="Assignment Objective"></g:Label>
		</div>
		<div>
			<g:TextArea ui:field="assignmentObjective" width="800px" height="130px"></g:TextArea>
		</div>
		</g:HTMLPanel>
	</g:VerticalPanel>
		
	<g:VerticalPanel ui:field="activeObjUI"> 
		<g:HTMLPanel>
		<div>
			<g:Label text="Activity Objective"></g:Label>
		</div>
		<div>
			<g:TextArea ui:field="activityObjective" width="800px" height="130px"></g:TextArea>
		</div>
		</g:HTMLPanel>
		
	</g:VerticalPanel>
		
			
	<g:VerticalPanel ui:field="processUnderstandingUI"> 
		<g:HTMLPanel>
		<div>
			<g:Label text="Underlaying Process Identification"></g:Label>
		</div>
		
		<div>
			<g:TextArea ui:field="process" width="800px" height="130px"></g:TextArea>
		</div>
		<div>
		
		<g:HorizontalPanel>
			
<!-- 			<g:Button ui:field="attachment">Attachment</g:Button> -->
		
		</g:HorizontalPanel>

		</div>
	</g:HTMLPanel>		
			
	</g:VerticalPanel>
	
	<g:Button ui:field="submit">Save</g:Button>

</g:HTMLPanel>
</ui:UiBinder>